{
  "name": "roo-hook-tester",
  "displayName": "Roo Hook Tester",
  "description": "Test extension for Roo Code hook system v6 - Fixed content parsing",
  "version": "0.0.6",
  "publisher": "roo-code-hook",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": ["Other"],
  "activationEvents": ["*"],
  "contributes": {
    "commands": [
      {
        "command": "roo-hook-tester.stop",
        "title": "Stop Roo Hook Server"
      },
      {
        "command": "roo-hook-tester.clear",
        "title": "Clear Roo Hook Logs"
      },
      {
        "command": "roo-hook-tester.start",
        "title": "Start Roo Hook"
      },
      {
        "command": "roo-hook-tester.status",
        "title": "Roo Hook Status"
      },
      {
        "command": "roo-hook-tester.toggleAutoStart",
        "title": "Toggle Auto Start Hook"
      }
    ]
  },
  "main": "./out/extension-v4.js",
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "@types/express": "^4.17.17",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "express": "^4.18.2"
  }
}